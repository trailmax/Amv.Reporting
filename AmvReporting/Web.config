<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <add key="RavenDataPath" value="d:\Raven\Data\"/>
    <add key="BackupPath" value="d:\Raven\Backups\"/>
    <!--Turn on RavenDB studio. Not recommended in production. 
    If enabled go to "~/db/" to see the studio-->
    <add key="EnableRavenStudio" value="true"/>
    <!--Comma-separated list of group names that should be able to modify reports
    User must be in at least one of these groups. If you want grouping to work as AND operator, speak to Max-->
    <add key="AdministratorRoleNames" value=""/>
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5.1"/>
    <httpRuntime targetFramework="4.5" enableVersionHeader="false"/>
    <authentication mode="Windows"/>
    <customErrors mode="RemoteOnly" defaultRedirect="~/Content/Errors/page500.aspx" redirectMode="ResponseRewrite">
      <error statusCode="404" redirect="~/Content/Errors/page404.aspx"/>
    </customErrors>
    <identity impersonate="false"/>
  </system.web>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <remove name="X-Powered-By"/>
      </customHeaders>
    </httpProtocol>
    <handlers>
      <add name="Browser Link for HTML" path="*.html" verb="*" type="System.Web.StaticFileHandler, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" resourceType="File" preCondition="integratedMode"/>
    </handlers>
    <staticContent>
      <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="30.00:00:00"/>
      <!-- Mime mappings courtesy: http://stackoverflow.com/a/7374640/476786 Adding a mapping without trying to remove it first throws an error if it already exists-->
      <remove fileExtension=".woff"/>
      <mimeMap fileExtension=".woff" mimeType="application/font-woff"/>
    </staticContent>
  </system.webServer>
